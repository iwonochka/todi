<% @users.each do |user| %>
<a href="<%profile_path(user)%>">
<div class="profile-mini-card" data-id="1">
<% if user.photo.attached?%>
  <%=cl_image_tag user.photo.key, class:"avatar-large"%>
<%end%>
  <div class="profile-mini-card-infos">
    <h2><%=user.username%></h2>
    <p><%=user.description%></p>
  </div>
</div>
</a>
<% end %>
